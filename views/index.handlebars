<div class="card topCard">
    <div class="card-body text-center">
        Chess Openings Scraper
    </div>
    <button type="submit" class="scrapeOpenings btn-success">Scrape Openings</button>
    <button type="submit" class="savedOpenings btn-info">Saved Openings</button>
</div>

<div class="container card2 centered">
    <div class="card">
        <div class="card-body"> Openings</div>
        <div class="openings">
            <ol>
                {{#each openings}}
                {{> openings/openings-block}}
                {{/each}}
            </ol>
        </div>
    </div>
</div>